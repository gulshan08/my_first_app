<%= form_for @account, :html => {:class => "form-horizontal center", multipart: true} do |f| %>
      <div class="form-group">
        <%= f.label :address,"address", class: "col-md-4 control-label" %>
        <div class="col-md-8">
          <%= f.text_field :address, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :mobile_number,"mobile_number", class: "col-md-4 control-label" %>
        <div class="col-md-8">
          <%= f.text_field :mobile_number, class: "form-control", required: true %>
        </div>
      </div>

      <div class="form-group">
        <%= f.label :upload_by_paperclip,"upload_by_paperclip", class: "col-md-4 control-label" %>
        <div class="col-md-8">
          <%= f.file_field :upload_by_paperclip, class: "form-control" %>
        </div>
      </div>
      <%= f.submit "Submit" , :autofocus => true,class: "btn btn-success" %>  
 <% end %>